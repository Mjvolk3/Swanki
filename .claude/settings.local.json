{
  "permissions": {
    "additionalDirectories": ["../Swanki_Data", "../iBioFoundry-AI"],
    "allow": [
      "Read(**)",
      "Search(**)",
      "Glob(**)",
      "Grep(**)",
      "Write(**/*.py)",
      "Write(**/*.md)",
      "Write(**/*.sh)",
      "Edit(**/*.py)",
      "Edit(**/*.md)",
      "Edit(**/*.sh)",
      "Bash(mkdir:*)",
      "Bash(ls:*)",
      "Bash(python:*)",
      "Bash(python3:*)",
      "Bash(wc:*)",
      "Bash(dendron-cli:*)",
      "Bash(find:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(grep:*)",
      "Bash(code:*)",
      "Bash(git add:*)",
      "Bash(echo:*)"
    ],
    "deny": [
      "Bash(rm *)",
      "Bash(sudo *)",
      "Bash(chmod *)",
      "Bash(cat *.env*)",
      "Bash(head *.env*)",
      "Bash(tail *.env*)",
      "Bash(echo *.env*)",
      "Read(**/.env)",
      "Read(**/*.env)",
      "Read(**/*.pem)",
      "Read(~/.ssh/**)",
      "Edit(**/.env)",
      "Edit(**/*.env)"
    ]
  },
  "enableAllProjectMcpServers": false,
  "outputStyle": "default"
}
